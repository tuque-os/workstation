{
  "policies": {
    "DisableAppUpdate": true,
    "HttpsOnlyMode": "force_enabled",
    "OfferToSaveLoginsDefault": false,
    "Extensions": {
        "Install": [
            "https://addons.mozilla.org/firefox/downloads/latest/proton-pass/latest.xpi",
            "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi",
            "https://addons.mozilla.org/firefox/downloads/latest/darkreader/latest.xpi",
            "https://addons.mozilla.org/firefox/downloads/latest/libredirect/latest.xpi",
            "https://addons.mozilla.org/firefox/downloads/latest/multi-account-containers/latest.xpi",
            "https://addons.mozilla.org/firefox/downloads/latest/protondb-for-steam/latest.xpi"
        ]
    },
    "ExtensionSettings": {
      "ProtonPass": {
        "default_area": "navbar",
        "installation_mode": "normal_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/proton-pass/latest.xpi",
        "private_browsing": true,
        "temporarily_allow_weak_signatures": false
      },
      "uBlock0": {
        "default_area": "navbar",
        "installation_mode": "normal_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi",
        "private_browsing": true,
        "temporarily_allow_weak_signatures": false
      },
      "DarkReader": {
        "default_area": "navbar",
        "installation_mode": "normal_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/darkreader/latest.xpi",
        "private_browsing": true,
        "temporarily_allow_weak_signatures": false
      },
      "LibRedirect": {
        "default_area": "navbar",
        "installation_mode": "normal_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/libredirect/latest.xpi",
        "private_browsing": true,
        "temporarily_allow_weak_signatures": false
      },
      "MultiAccountContainers": {
        "default_area": "navbar",
        "installation_mode": "normal_installed",
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/multi-account-containers/latest.xpi",
        "private_browsing": true,
        "temporarily_allow_weak_signatures": false
      }
    },
    "Hopepage": {
      "URL": "about:home",
      "StartPage": "homepage"
    },
    "FirefoxHome": {
      "Search": false,
      "TopSites": false,
      "SponsoredTopSites": false,
      "Highlights": false,
      "Pocket": false,
      "SponsoredPocket": false,
      "Snippets": false,
      "Locked": true
    },
    "FirefoxSuggest": {
      "ImproveSuggest": false,
      "SponsoredSuggestions": false,
      "WebSuggestions": false
    },
    "SearchEngines": {
      "Default": "DuckDuckGo",
      "DefaultPrivate": "DuckDuckGo",
      "Remove": [
        "Amazon.ca",
        "Amazon.com",
        "Bing",
        "eBay",
        "Google"
      ]
    },
    "Preferences": {
      "browser.tabs.closeWindowWithLastTab": {
        "Value": false,
        "Status": "default"
      },
      "browser.urlbar.suggest.topsites": {
        "Value": false,
        "Status": "default"
      },
      "browser.urlbar.suggest.quickactions": {
        "Value": false,
        "Status": "default"
      },
      "browser.urlbar.suggest.engines": {
        "Value": false,
        "Status": "default"
      },
      "browser.toolbars.bookmarks.visibility": {
        "Value": "never",
        "Status": "default"
      },
      "browser.tabs.hoverPreview.showThumbnails": {
        "Value": false,
        "Status": "default"
      }
    },
    "EnableTrackingProtection": {
      "Category": "strict",
      "Cryptomining": true,
      "EmailTracking": true,
      "Fingerprinting": true,
      "Value": true
    },
    "GenerativeAI": {
      "Enabled": false
    },
    "DontCheckDefaultBrowser": true
  }
}
